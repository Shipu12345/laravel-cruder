{
    "name": "shipus/laravel-cruder",
    "version": "1.0.0",
    "description": "make auto crud's from command",
    "keywords": [
        "shipu",
        "laravel",
        "laravel-cruder"
    ],
    "homepage": "https://github.com/Shipu12345/laravel-cruder.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Shipu",
            "email": "shipankar.shipu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0",
        "cocur/slugify": "^4.4",
        "illuminate/support": "^10.21",
        "illuminate/console": "^10.21",
        "illuminate/database": "^10.21",
        "illuminate/http": "^10.21"
    },
    "autoload": {
        "psr-4": {
            "Shipu\\Cruder\\": "src/Cruder/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shipu\\Cruder\\CrudableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
